---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <url>
    <loc>https://festivitymishra.github.io/</loc>
    <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
    <changefreq>weekly</changefreq>
    <priority>1.0</priority>
  </url>
  <url>
    <loc>https://festivitymishra.github.io/about/</loc>
    <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
    <changefreq>monthly</changefreq>
    <priority>0.9</priority>
  </url>
  <url>
    <loc>https://festivitymishra.github.io/projects/</loc>
    <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
    <changefreq>weekly</changefreq>
    <priority>0.9</priority>
  </url>
  <url>
    <loc>https://festivitymishra.github.io/contact/</loc>
    <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
    <changefreq>monthly</changefreq>
    <priority>0.8</priority>
  </url>
  <url>
    <loc>https://festivitymishra.github.io/resume/</loc>
    <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
    <changefreq>monthly</changefreq>
    <priority>0.8</priority>
  </url>
  {% for project in site.projects %}
  <url>
    <loc>https://festivitymishra.github.io{{ project.url }}</loc>
    <lastmod>{{ project.date | default: site.time | date: "%Y-%m-%d" }}</lastmod>
    <changefreq>monthly</changefreq>
    <priority>0.7</priority>
  </url>
  {% endfor %}
  {% for post in site.posts %}
  <url>
    <loc>https://festivitymishra.github.io{{ post.url }}</loc>
    <lastmod>{{ post.date | date: "%Y-%m-%d" }}</lastmod>
    <changefreq>monthly</changefreq>
    <priority>0.6</priority>
  </url>
  {% endfor %}
</urlset>

